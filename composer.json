{
  "name": "peibin/laravel-di",
  "description": "Peibin di component for Laravel.",
  "license": "MIT",
  "keywords": [
    "php",
    "laravel",
    "annotation"
  ],
  "authors": [
    {
      "name": "Peibin Zhu",
      "email": "zhupeibin1207@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "nikic/php-parser": "^4.1",
    "doctrine/annotations": "^1.13",
    "laravel/framework": "^9.2",
    "php-di/phpdoc-reader": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "PeibinLaravel\\Di\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PeibinLaravelTest\\Di\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PeibinLaravel\\Di\\DiServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
